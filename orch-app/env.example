# Orchestra API Configuration
# Copy this file to .env and update with your actual values

# API Base URL (required)
API_BASE_URL=https://your-orchestra-api.com/api

# API Authentication (optional)
API_KEY=your_api_key_here
AUTH_TOKEN=your_bearer_token_here

# Development overrides (optional)
# These will only be used in development mode
DEV_API_BASE_URL=http://localhost:3000/api
DEV_API_KEY=dev_api_key
DEV_AUTH_TOKEN=dev_auth_token

# Timeout Configuration (optional)
REQUEST_TIMEOUT=30000
POLLING_INTERVAL=2000
MAX_POLLING_TIME=300000
